<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:SampleData="clr-namespace:Expression.Blend.SampleData.SampleDataImageSource"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="ImageAnnotation_beta1_0.App"
	StartupUri="MainWindow.xaml">
	<Application.Resources>
		<!-- 应该在此定义应用程序级的资源。-->
        <ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Microsoft.Expression.Prototyping.SketchControls;Component/ScrollViewerStyles.xaml"/>
				<ResourceDictionary Source="ImageAnnotation-beta1.0Screens/SketchStyles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<SampleData:SampleDataImageSource x:Key="SampleDataImageSource" d:IsDataSource="True"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>