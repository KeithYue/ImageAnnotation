<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="ImageAnnotation_beta1_0.PhotoEditor"
	x:Name="Window"
	Title="PhotoEditor"
	Width="800" Height="600" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">

	<Grid x:Name="LayoutRoot">
		<TabControl Margin="16,8,24,21">
			<TabItem Header="美化" FontSize="18.667">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
			<TabItem Header="边框" FontSize="18.667">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
			<TabItem Header="饰品" FontSize="18.667">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
			<TabItem Header="场景" FontSize="18.667">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
			<TabItem Header="文字" FontSize="18.667">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
		</TabControl>
		<Grid Margin="120,48,32,37" Panel.ZIndex="5">
			<Grid.RowDefinitions>
				<RowDefinition Height="0.089*"/>
				<RowDefinition Height="0.667*"/>
				<RowDefinition Height="0.244*"/>
			</Grid.RowDefinitions>
			<Image Margin="104,12.16,128,8" Grid.Row="1" Width="400" Height="300"/>
		</Grid>
	</Grid>
</Window>